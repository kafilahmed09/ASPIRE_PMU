@model IEnumerable<ASPIRE.Models.Domain.MasterSetup.Provience>

@{
    ViewData["Title"] = "Index";    
    ViewBag.pageTitle = "Provience";
    Layout = "~/Views/_Shared/_Layout.cshtml";
}

<h2>List of Provience</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Code)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Code)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.ProvienceId">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.ProvienceId">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.ProvienceId">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>


