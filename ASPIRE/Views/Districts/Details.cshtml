@model ASPIRE.Models.Domain.MasterSetup.District

@{
    ViewData["Title"] = "Details";
    ViewBag.pageTitle = "Division";
    ViewBag.Title = "District";
    ViewBag.pTitle = "District";
    Layout = "~/Views/_Shared/_Layout.cshtml";
}

<div class="card">
    <div class="card-body">
        <div class="alert alert-info" role="alert">
            <div class="row">
                <div class="col-sm-6">
                    <h4 class="mt-2 card-title">District Detail</h4>
                </div>
                <div class="col-sm-6">
                    <div class="text-sm-right">
                        <a asp-action="Index" type="button" class="btn btn-success btn-rounded waves-effect waves-light mr-2">Back to List</a>
                    </div>
                </div><!-- end col-->
            </div>
        </div>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Name)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Name)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Code)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Code)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Description)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Description)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Division)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Division.Name)
            </dd>
        </dl>
    </div>
</div>
